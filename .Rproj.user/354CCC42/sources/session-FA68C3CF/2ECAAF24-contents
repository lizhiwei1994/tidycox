# install.packages("hexSticker")
library(hexSticker)

picture <- 'man/figures/pic.png'
s = sticker(picture, package="TidyCox",
            p_size=20, s_x=1, s_y=.77, s_width=.6,
            h_fill="#ffffff",
            h_color='#bc0008',
            p_color='#bc0008',
            h_size = 1,
        filename="man/figures/logo.png")

plot(s)

